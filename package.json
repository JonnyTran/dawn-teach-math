{
  "name": "dawn-teach-math",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "python -m uvicorn api.index:app --host 0.0.0.0 --port 8000 --reload",
    "dev": "concurrently \"vite --port 3000\" \"npm run fastapi-dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --watch=false",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "pre-commit": "lint-staged && npm run test:unit"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@formkit/vue": "^0.17.4",
    "babel-runtime": "^6.26.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-simple-search-dropdown": "^1.0.1"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.4",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vercel/node": "^3.0.1",
    "@vercel/python": "^4.0.0",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.2.7",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.11.0",
    "eventsource": "^2.0.2",
    "flowbite": "^1.6.6",
    "flowbite-vue": "^0.0.14",
    "husky": "^8.0.3",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "markdown-it": "^13.0.1",
    "mitt": "^3.0.1",
    "nanoid": "^5.0.1",
    "pinia": "^2.1.6",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vercel": "^24.2.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.4",
    "vue-cookies": "^1.8.3",
    "vue-tailwind-datepicker": "^1.4.6",
    "vue3-beautiful-chat": "^3.3.1"
  }
}
