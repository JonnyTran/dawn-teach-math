{
    "version": 2,
    "public": false,
    "devCommand": "npm run dev",
    "builds": [
        { 
            "src": "/api/index.py", 
            "use": "@vercel/python"
        }
    ],
    "routes": [
        { "src": "^(?!\\/api\\/|\\/docs$|\\/openapi\\.json$).*", "dest": "" },
		{ "src": "/api/(.*)", "dest": "api/index.py" },
		{ "src": "/docs", "dest": "api/index.py" },
		{ "src": "/openapi.json", "dest": "api/index.py" }
	]
}