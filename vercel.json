{
    "builds": [{
        "src": "api/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "100mb"
        }
    }],
    "rewrites": [
        { "source": "/(.*)", "destination": "/" },
        { "source": "/api/:path*", "destination": "api/wsgi.py" }
    ]
}