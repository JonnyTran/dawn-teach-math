{
    "builds": [
        { 
            "src": "/api/index.py", 
            "use": "@vercel/python", 
            "config": { 
                "maxLambdaSize": "100mb",
                "cmd": "npm run fastapi-dev"
            }
        }
    ],
    "rewrites": [
        { "source": "/api/:path*", "destination": "http://127.0.0.1:8000/api/:path*"}
    ]
}